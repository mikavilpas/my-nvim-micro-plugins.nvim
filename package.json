{
  "name": "@mikavilpas/my-nvim-micro-plugins.nvim",
  "version": "1.0.0",
  "private": true,
  "description": "Some utilities that I use in my nvim config",
  "keywords": [],
  "license": "MIT",
  "author": "",
  "workspaces": [
    "integration-tests"
  ],
  "scripts": {
    "clean-install:all": "npm clean-install --workspaces --include-workspace-root",
