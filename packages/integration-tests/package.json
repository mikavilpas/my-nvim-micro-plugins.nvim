    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-oxlint": "1.32.0",
    "oxlint-tsgolint": "0.8.6",
    "tsx": "4.21.0",
    "type-fest": "5.3.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.49.0",
    "wait-on": "9.0.3",
    "zod": "4.1.13"
